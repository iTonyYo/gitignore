{
  "bin": {
    "latest-gitignore": "esm/cli.js"
  },
  "main": "esm/latestGitignore.js",
  "scripts": {
    "start": "DEBUG=LG:* npx babel-node ./dev",
    "cli": "DEBUG=LG:* npx babel-node ./src/cli macOS Windows Linux Node VisualStudioCode SublimeText CVS Diff Vim TortoiseGit -o .",
    "build": "npx babel-node ./scripts/clean-built && npx babel-node ./scripts/build",
    "clean": "npx babel-node ./scripts/clean",
    "license": "npx babel-node ./scripts/license",
    "reprepare": "npx babel-node ./scripts/re-prepare",
    "lint": "npx eslint --cache ./src/**/*.js",
    "changelog": "npx auto-changelog -p"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "files": [
    "src/**/*.js",
    "esm/**/*.js"
  ],
  "name": "@oopsunome/latest-gitignore",
  "description": "添加 / 更新 `.gitignore`。直接从 `github/gitignore·` 项目 `master` 分支获取模板，而非 `gitignore.io`。",
  "dependencies": {
    "cache-manager": "^2.9.0",
    "cache-manager-fs": "^1.0.7",
    "callsites": "^3.1.0",
    "camelcase-keys": "^5.1.0",
    "chalk": "^2.4.2",
    "debug": "^4.1.1",
    "fast-glob": "^2.2.6",
    "got": "^9.6.0",
    "has-ext": "^1.0.1",
    "is-reachable": "^3.0.0",
    "lodash": "^4.17.11",
    "meow": "^5.0.0",
    "p-map": "^2.1.0",
    "redent": "^2.0.0",
    "update-notifier": "^2.5.0",
    "write-file-atomic": "^2.4.2"
  },
  "private": false,
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "auto-changelog": "^1.12.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "execa": "^1.0.0",
    "license-checker": "^25.0.1",
    "listr": "^0.14.3",
    "make-dir": "^3.0.0",
    "trash": "^5.2.0"
  },
  "keywords": [
    "git",
    "gitignore",
    "初始化",
    "init",
    "脚手架",
    "scaffold",
    "生成器",
    "generator",
    "工具",
    "tool",
    "cli",
    "oopsunome"
  ],
  "version": "1.0.0",
  "author": {
    "name": "iyowei",
    "email": "swzyocowboy@icloud.com",
    "url": "https://github.com/iTonyYo"
  },
  "repository": {
    "url": "git@github.com:iTonyYo/latest-gitignore.git",
    "type": "git"
  },
  "homepage": "https://github.com/iTonyYo/latest-gitignore",
  "bugs": "https://github.com/iTonyYo/latest-gitignore/issues",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
